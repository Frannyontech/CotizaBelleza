[run]
source = .
omit = 
    */migrations/*
    */__pycache__/*
    */venv/*
    */node_modules/*
    manage.py
    */wsgi.py
    */asgi.py
    */settings.py
    */urls.py
    */admin.py
    */apps.py
    */celery_app.py
    */celery_etl.py
    */setup.py
    */conftest.py
    */test_*.py
    */tests.py
    */__init__.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    class .*\bProtocol\):
    @(abc\.)?abstractmethod
